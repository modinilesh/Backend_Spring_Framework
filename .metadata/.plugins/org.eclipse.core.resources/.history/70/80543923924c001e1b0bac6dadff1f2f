package Sorting;

public class InsertionSort {
	
	public static void insertionSort(int[] arr) {
		for(int i=1; i<arr.length; i++) {
			int temp = arr[i];
			int j = i-1;
			while(arr[i] < arr[j]) {
				j--;
			}
			arr[i] = arr[j];
			arr[j] = temp;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
